<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dragus.ellouzeandpartners.dragus" >


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:name="com.dragus.ellouzeandpartners.app.AppController"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.dragus.ellouzeandpartners.activities.LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dragus.ellouzeandpartners.activities.ListPlansActivity"
            android:label="@string/title_activity_list_plans"
            android:screenOrientation="landscape">
        </activity>
        <activity
            android:name="com.dragus.ellouzeandpartners.activities.DetailsPlanActivity"
            android:label="@string/title_activity_details_plan"
            android:screenOrientation="landscape">
        </activity>
    </application>

</manifest>
